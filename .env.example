# GCP Project
GOOGLE_CLOUD_PROJECT=your-gcp-project-id
GOOGLE_CLOUD_REGION=us-east1

# Terraform variables (used via TF_VAR_ prefix)
TF_VAR_project_id=your-gcp-project-id
TF_VAR_billing_account_id=XXXXXX-XXXXXX-XXXXXX
TF_VAR_owner_email=your-email@example.com

# RAG Service (Phase 3)
LABSIGHT_GCP_PROJECT=your-gcp-project-id
LABSIGHT_GCP_REGION=us-east1
LABSIGHT_CHROMADB_URL=https://chromadb-dev-xxxx.a.run.app
LABSIGHT_LLM_PROVIDER=vertex_ai
LABSIGHT_OPENROUTER_API_KEY=sk-or-xxxx
LABSIGHT_BIGQUERY_QUERY_LOG_TABLE=
LABSIGHT_RETRIEVAL_CANDIDATE_K=20
LABSIGHT_RETRIEVAL_FINAL_K=5
LABSIGHT_RERANK_ENABLED=false
LABSIGHT_RERANKER_MODEL=cross-encoder/ms-marco-MiniLM-L-6-v2
LABSIGHT_RERANKER_MAX_CANDIDATES=30
# Optional for Phase 6 eval scripts
LABSIGHT_RETRIEVAL_EVAL_BQ_DATASET=platform_observability_dev

# Agent (Phase 4) — empty disables the agent, all queries use RAG only
LABSIGHT_BIGQUERY_METRICS_DATASET=

# SQL policy — "strict" (default) requires fully-qualified table names from
# the allowed list; "flex" allows unqualified and table-less queries (dev use).
# Strict mode fails startup if allowed tables is empty.
LABSIGHT_SQL_POLICY_MODE=strict
LABSIGHT_SQL_ALLOWED_TABLES=uptime_events,resource_utilization,service_inventory

# Upload + Dashboard (Phase 5) — empty disables the endpoints
LABSIGHT_GCS_UPLOADS_BUCKET=
LABSIGHT_BIGQUERY_OBSERVABILITY_DATASET=

# Frontend auth mode (Phase 5B)
# "id_token" — direct Cloud Run invocation via google-auth-library (Phase 5A default)
# "api_key" — API Gateway route with x-api-key header (Phase 5B with domain set)
BACKEND_AUTH_MODE=id_token
BACKEND_API_KEY=

# Phase 5B: IAP + API Gateway (optional — enabled when domain is set)
TF_VAR_domain=
TF_VAR_iap_members=["user:your-email@gmail.com"]
TF_VAR_iap_oauth_client_id=
TF_VAR_iap_oauth_client_secret=
TF_VAR_frontend_public=false

# Phase 6 retrieval tuning (Terraform -> Cloud Run env)
TF_VAR_retrieval_candidate_k=20
TF_VAR_retrieval_final_k=5
TF_VAR_rerank_enabled=false
TF_VAR_reranker_model=cross-encoder/ms-marco-MiniLM-L-6-v2
TF_VAR_reranker_max_candidates=30
